kind: "BuildConfig"
apiVersion: "v1"
metadata:
  name: "testbuild"
spec:
  source: 
    type: "Git"
    git:
      uri: "https://github.com/avenging/openshiftbuild"
      ref: "master"
  strategy: 
    type: "Docker"
  output: 
    to:
      kind: "DockerImage"
      name: "http://172.30.1.1:5000/myapp/hybris:latest"
